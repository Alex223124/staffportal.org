- title "DMPD Weekly Support Raising Development Chart"
.white
  .container
    %h3.page-header
      Appointment Set Record
      .pull-right
        = link_to "Delete ASR", staff_dmpd_stats_appointment_set_record_path(@appointment_set_record), class: "btn btn-danger", method: :delete, :confirm => "Are you sure you want to delete this ASR?"
    = semantic_form_for @appointment_set_record, url: staff_dmpd_stats_appointment_set_record_path(@appointment_set_record), builder: FormtasticBootstrap::FormBuilder do |f|
      = render :partial => "form", :locals => { :f => f }
